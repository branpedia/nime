{
  "version": 2,
  "builds": [
    {
      "src": "api/search.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/anime.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/stream.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/anime/(.*)",
      "dest": "/api/anime.js"
    },
    {
      "src": "/api/stream",
      "dest": "/api/stream.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/search.js": {
      "maxDuration": 60,
      "memory": 3008
    },
    "api/anime.js": {
      "maxDuration": 60,
      "memory": 3008
    },
    "api/stream.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
